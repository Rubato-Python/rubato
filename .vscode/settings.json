{
    "restructuredtext.confPath": "${workspaceFolder}/docs/source",
    "python.linting.pylintEnabled": true,
    "python.linting.enabled": true,
    "cSpell.words": [
        "autoclass",
        "automodule",
        "blit",
        "cieled",
        "cmax",
        "cmin",
        "cpyright",
        "Elnekave",
        "furo",
        "ivar",
        "KEYUP",
        "LALT",
        "LCTRL",
        "LEFTPAREN",
        "lerp",
        "LMETA",
        "LSUPER",
        "maxdex",
        "milli",
        "mindex",
        "NUMLOCK",
        "PAGEUP",
        "pmath",
        "RALT",
        "RCTRL",
        "RIGHTPAREN",
        "rigidbody",
        "RMETA",
        "RSUPER",
        "rtype",
        "SCROLLOCK",
        "Sevenson",
        "SRCALPHA",
        "SYSREQ",
        "Tomer",
        "Yamm"
    ],
    "python.formatting.provider": "yapf",
    "python.linting.pylintCategorySeverity.refactor": "Information",
    "githubPullRequests.ignoredPrBranches": ["main"],
    "python.linting.ignorePatterns": ["**/site-packages/**/*.py", ".vscode/*.py", "demo/**/*", "docs/**/*"],
    "githubPullRequests.ignoredPullRequestBranches": ["main"],
    "files.associations": {
        "cstdint": "c",
        "cmath": "cpp",
        "complex": "cpp",
        "random": "cpp",
        "atomic": "cpp",
        "bit": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "compare": "cpp",
        "concepts": "cpp",
        "cstddef": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "ctime": "cpp",
        "cwchar": "cpp",
        "exception": "cpp",
        "initializer_list": "cpp",
        "ios": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "iterator": "cpp",
        "limits": "cpp",
        "memory": "cpp",
        "new": "cpp",
        "ostream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "system_error": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "typeinfo": "cpp",
        "utility": "cpp",
        "xfacet": "cpp",
        "xiosbase": "cpp",
        "xlocale": "cpp",
        "xlocinfo": "cpp",
        "xlocnum": "cpp",
        "xmemory": "cpp",
        "xstddef": "cpp",
        "xstring": "cpp",
        "xtr1common": "cpp",
        "xutility": "cpp"
    },
    "C_Cpp.formatting": "vcFormat",
    "C_Cpp.vcFormat.newLine.beforeElse": false,
    "C_Cpp.vcFormat.newLine.beforeCatch": false,
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.block": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.lambda": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.namespace": "sameLine",
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.type": "sameLine",
    "C_Cpp.vcFormat.space.afterCastCloseParenthesis": true,
    "editor.formatOnSave": true,
    "todo-tree.tree.showBadges": true,
    "todo-tree.filtering.excludeGlobs": ["**/node_modules", "**/static"],
    "python.analysis.completeFunctionParens": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "autoDocstring.customTemplatePath": "autodocstringtemplate.mustache",
    "python.analysis.typeCheckingMode": "basic"
}
